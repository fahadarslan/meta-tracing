<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- This manifest defines the configuration for the MEL 2014.05-based Lennox LCC builds. -->

  <!-- Derive from the MEL 2014.05 release configuration as a base -->
  <include name="2014.05/mel_2014.05_frozen.xml" />

  <!-- Remove layers not part of lennox project -->

  <remove-project name="meta-ro-rootfs.git" />
  <remove-project name="meta-ti-glsdk.git" />
  <remove-project name="meta-arago-glsdk.git" />
  <remove-project name="meta-mentor-glsdk.git" />
  <remove-project name="meta-omap5-evm.git" />
  <remove-project name="meta-dra7xx-evm.git" />
  <remove-project name="mel-x86-pcsim.git" />
  <remove-project name="mel-meta-atp.git" />
  <remove-project name="meta-fsl-ppc.git" />
  <remove-project name="meta-intel.git" />
  <remove-project name="meta-ivi.git" />
  <remove-project name="meta-minnow.git" />
  <remove-project name="meta-ti.git" />
  <remove-project name="meta-virtualization.git" />
  <remove-project name="mel-meta-fsl-networking.git" />
  <remove-project name="meta-qt5-mel.git" />
  <remove-project name="meta-qt5/meta-qt5.git" />
  <remove-project name="meta-tracing.git" />

  <!-- Unlock scripts, docs, and updates-->
  <remove-project name="mel-scripts.git" />
  <remove-project name="mel-docs.git" />
  <remove-project name="mel-updates.git" />
  <project name="mel-scripts.git" path="scripts" revision="release/2014.05" />
  <project name="mel-docs.git" path="docs" revision="release/2014.05" />
  <project name="mel-updates.git" path="updates" revision="release/2014.05" />

  <!-- New repositories for this project -->
  <project name="meta-lennox-lcc.git" path="meta-lennox-lcc" revision="master" />

</manifest>
